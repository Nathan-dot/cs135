#reader(lib "htdp-beginner-reader.ss" "lang")((modname trace-test) (read-case-sensitive #t) (teachpacks ()) (htdp-settings #(#t constructor mixed-fraction #f #t none #f () #t)))
(require htdp-trace)

;; This DrRacket file tests whether you have installed the
;; uwaterloo-racket-tools package correctly

;; (polynomial x) produces a polynomial evaluated at x
(define/trace (polynomial x)
  (+ (sqr x) (* 3 x) 8))

(polynomial -3.2)
(polynomial 8)

